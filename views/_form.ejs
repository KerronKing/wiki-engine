<div class="form-group">
  <label for="title">Enter page title:</label>
  <input class="form-control" type="text" name="title" id="title" value="<%= wikiPage.title %>" required>
</div>
<div class="form-group">
  <label for="content">Enter page content:</label>
  <textarea onchange="preview" class="form-control" name="content" id="content" rows="10" cols="50" required><%= wikiPage.content %></textarea>
</div>
<button type="submit" class="btn btn-primary">Submit</button>
